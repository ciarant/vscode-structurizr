{
    "name": "vscode-structurizr",
    "displayName": "Structurizr",
    "description": "Structurizr DSL syntax highlighting",
    "version": "0.0.9",
    "publisher": "ciarant",
    "license": "MIT",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "icon": "images/icon.png",
    "contributes": {
        "languages": [
            {
                "id": "structurizr",
                "aliases": [
                    "Structurizr",
                    "structurizr"
                ],
                "extensions": [
                    "dsl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "structurizr",
                "scopeName": "source.structurizr",
                "path": "./syntaxes/structurizr.tmLanguage.json"
            }
        ]
    },
    "bugs": "https://github.com/ciarant/vscode-structurizr/issues",
    "homepage": "https://github.com/ciarant/vscode-structurizr/blob/master/README.md",
    "repository": {
        "type": "git",
        "url": "https://github.com/ciarant/vscode-structurizr.git"
    },
    "keywords": [
        "structurizr",
        "architecture",
        "C4 Model"
    ]
}
